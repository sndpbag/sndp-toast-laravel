{
    "name": "sndpbag/laravel-toast",
    "description": "A beautiful and feature-rich toast notification package for Laravel",
    "version": "1.0.0",
    "keywords": [
        "laravel",
        "toast",
        "notification",
        "alert",
        "message",
        "flash",
        "sndpbag"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "sndp bag",
            "email": "sndpbagg@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2|^8.3",
         "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "sndpbag\\LaravelToast\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "sndpbag\\LaravelToast\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "sndpbag\\LaravelToast\\ToastServiceProvider"
            ],
            "aliases": {
                "Toast": "sndpbag\\LaravelToast\\Facades\\Toast"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}